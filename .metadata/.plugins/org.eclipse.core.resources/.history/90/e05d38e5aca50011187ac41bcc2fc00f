package csg;

import org.junit.Test;

import primitives.Surface;

public class Test_CSGTree
{
	@Test(expected=NullPointerException.class)
	public void TestConstructor_NullSurface_ExpectNullPointerException()
	{
		Surface s = null;
		
		new CSGTree(new CSGNode(s));
	}
	
	public void TestConstructor_OperationNullChildren_ExpectException()
	{
		CSGNode node = new CSGNode(Operation.BOUNDED_BY);
	}
}
