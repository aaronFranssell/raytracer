package csg.object.tree.factory;

import csg.object.tree.CSGNode;
import csg.object.tree.CSGTree;
import etc.RaytracerException;

public class CSGTreeFactoryImpl implements CSGTreeFactory
{

	@Override
	public CSGTree createTree(CSGNode node) throws RaytracerException
	{
		return new CSGTree(node);
	}

}
